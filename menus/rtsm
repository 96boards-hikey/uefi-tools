# RTSM menu
UEFI_WORKSPACE=/linaro/uefi/master/uefi-next.git

function build
{
	cd $UEFI_WORKSPACE
	uefi-build.sh $*
}

rtsm=(

	'rtsm_select_model'
	'rtsm_set_mmc'
	'rtsm_set_uefi_image'
	'rtsm_build_uefi'

    separator

	'rtsm_run'

    separator

    rtsm_enable_networking
    rtsm_disable_networking

	separator

	'sudo service FMNetwork start'
	'sudo service FMNetwork stop'
	'sudo service FMNetwork status'
	'setup_flexlm_tunnel'
)

TITLE_rtsm="RTSM"
PRE_ACTION_rtsm="rtsm_enable_networking"

